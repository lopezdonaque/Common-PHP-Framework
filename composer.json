{
  "name": "commonphp/commonphp",
  "description": "Common PHP Library",
  "type": "library",
  "require-dev":
  {
    "common-ant-buildlibs/phplint": "dev-master",
    "common-ant-buildlibs/debpkg": "dev-master",
    "common-ant-buildlibs/antdeb": "dev-master",
    "common-ant-buildlibs/svn": "dev-master",
    "common-ant-buildlibs/git": "dev-master",
    "phpunit/PHPUnit": "3.7.*",
    "apigen/apigen": "2.8.0"
  },
  "autoload":
  {
    "psr-0":
    {
      "Common\\": "src/",
      "CommonTest\\": "tests/"
    }
  },
  "require":
  {
    "php": ">=5.3.0",
    "zendframework/zend-loader": "2.*",
    "zendframework/zend-file": "2.*",
    "zendframework/zend-mail": "2.*",
    "monolog/monolog": "1.*",
    "doctrine/common": "2.3.*"
  },
  "suggest":
  {
    "doctrine/orm": "Required by some classes"
  }
}
