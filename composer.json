{
  "name": "commonphp/commonphp",
  "description": "Common PHP Library",
  "type": "library",

  "version": "1.0.0",

  "repositories":
  [
    {
      "type": "composer",
      "url": "http://packages.zendframework.com/"
    }
  ],

  "require-dev":
  {
    "phpunit/PHPUnit": "3.7.*"
  },

  "autoload":
  {
    "psr-0":
    {
      "Common\\": "src/",
      "CommonTest\\": "tests/"
    }
  },

  "require":
  {
    "php": ">=5.3.0",
    "zendframework/zend-loader": "2.1.*",
    "zendframework/zend-file": "2.1.*",
    "zendframework/zend-mail": "2.1.*",
    "doctrine/common": "2.*",
    "monolog/monolog": "1.6.0"
  }
}
